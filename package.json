{
  "name": "devexp",
  "author": "Stanislav Sysoev <d4rkr00t@gmail.com> (http://sysoev.org)",
  "license": "MIT",
  "version": "0.1.0",
  "description": "Developer Experiments",
  "repository": {
    "url": "https://github.com/devexp-org/devexp"
  },
  "main": "app.js",
  "pre-commit": ["test", "eslint"],
  "precommit.silent": true,
  "scripts": {
    "test": "mocha --compilers js:babel/register -r ./tests/setup.js -R spec */**/__tests__/**.js",
    "test:heavy": "mocha --compilers js:babel/register -r ./tests/setup.js -R spec tests/heavy/**.js",
    "test:watch": "mocha --compilers js:babel/register -w -r ./tests/setup.js -R spec */**/__tests__/**.js",
    "_test": "npm run test --silent || true",
    "_test:heavy": "npm run test:heavy --silent || true",
    "eslint": "eslint .",
    "_eslint": "npm run eslint --silent || true",
    "coverage": "babel-node ./node_modules/.bin/isparta cover --report text --report html ./node_modules/.bin/_mocha -- -r ./tests/setup.js -R dot */**/__tests__/*.js tests/heavy/**.js",
    "_coverage": "npm run coverage --silent || true",
    "start": "nodemon start.js -e js,json,ejs,html --ignore node_modules/ --ignore public/ --ignore client/",
    "webpack:dev": "WEBPACK_DEV=1 webpack-dev-server --config ./.webpack/webpack.js --hot",
    "dev": "WEBPACK_DEV=1 ./node_modules/.bin/parallelshell 'npm run webpack:dev' 'npm run start'",
    "build": "webpack --config ./.webpack/webpack.js",
    "forever:start": "NODE_ENV=production forever start app.js",
    "forever:stop": "NODE_ENV=production forever stop app.js",
    "forever:restart": "NODE_ENV=production forever restart app.js",
    "forever:logs": "forever logs app.js",
    "postinstall": "scripts/postinstall.sh"
  },
  "dependencies": {
    "babel": "^5.8.21",
    "badgs": "^0.5.0",
    "body-parser": "^1.13.3",
    "ejs": "^2.3.3",
    "express": "^4.13.3",
    "github": "^0.2.4",
    "lodash": "^3.10.1",
    "minimatch": "^2.0.10",
    "moment": "^2.10.6",
    "mongoose": "^4.1.1",
    "node-xmpp-client": "1.0.0-alpha23",
    "object-assign": "^3.0.0",
    "proxy-express": "^1.1.2",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "recluster": "^0.4.0",
    "response-time": "^2.3.1",
    "setimmediate": "^1.0.2",
    "terror": "^1.3.0",
    "winston": "^1.0.1"
  },
  "devDependencies": {
    "alt": "^0.17.1",
    "autoprefixer-loader": "^2.0.0",
    "babel-eslint": "^4.0.5",
    "babel-loader": "^5.3.2",
    "chai": "^3.2.0",
    "chai-as-promised": "^5.1.0",
    "css-loader": "0.14.5",
    "eslint": "^1.1.0",
    "eslint-plugin-react": "^3.2.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "isparta": "^3.0.3",
    "istanbul": "^0.3.17",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.2.0",
    "nodemon": "^1.4.0",
    "parallelshell": "^1.2.0",
    "pre-commit": "^1.1.1",
    "proxyquire": "^1.6.0",
    "react": "^0.13.3",
    "react-hot-loader": "^1.2.8",
    "react-router": "^0.13.3",
    "react-timeago": "2.2.1",
    "sass-loader": "^1.0.2",
    "sinon": "^1.15.4",
    "style-loader": "^0.12.3",
    "webpack": "^1.10.1",
    "webpack-dev-server": "^1.10.1",
    "whatwg-fetch": "^0.9.0"
  }
}
