{
  "name": "devexp",
  "version": "0.1.0",
  "description": "DevExp",
  "main": "app.js",
  "scripts": {
    "postinstall": "scripts/postinstall.sh",
    "parallel": "./node_modules/.bin/parallelshell",
    "test": "echo 'TODO'",
    "webpack:dev": "WEBPACK_DEV=1 ./node_modules/.bin/webpack-dev-server --config ./.build/webpack.js --hot",
    "mongo": "mkdir -p data 2> /dev/null && mongod --dbpath ./data",
    "serve:dev": "nodemon app.js -e js,html --ignore __test__ --ignore public/ --ignore app/client/ --ignore node_modules",
    "dev:nowp": "npm run parallel -- 'npm run mongo' 'npm run serve:dev'",
    "dev": "WEBPACK_DEV=1 npm run parallel -- 'npm run webpack:dev' 'npm run mongo' 'npm run serve:dev'",
    "build": "./node_modules/.bin/webpack --config ./.build/webpack.js"
  },
