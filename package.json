    "github": "^0.2.4",
    "lodash": "^3.10.0",
    "mongoose": "^4.0.6",
    "proxy-express": "^1.1.2",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "react-timeago": "2.2.1",
    "response-time": "^2.3.1",
    "whatwg-fetch": "^0.9.0",
